<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:simpleType name="AddresseeIDTypeCode">
        <xs:annotation>
            <xs:documentation>A controlled value identifying a scheme from which an addressee identifier is taken.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="AgentAgentRelatorCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a relationship between a defined agent or agent class and a related agent or agent class.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="onixPL:IsA">
                <xs:annotation>
                    <xs:documentation>The agent belongs to the related agent class.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:IsAnyOf">
                <xs:annotation>
                    <xs:documentation>The agent is any of the related agents, or belongs to at least one of the related agent classes.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:IsAuthorizedRepresentativeOf">
                <xs:annotation>
                    <xs:documentation>The agent is authorized to act for the related agent or agent class.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:IsMemberOf">
                <xs:annotation>
                    <xs:documentation>The agent is a member of the related agent organization.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AgentIDTypeCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a scheme from which an agent identifier is taken.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="onixPL:CompanyRegistrationNumber">
                <xs:annotation>
                    <xs:documentation>A number under which a company is officially registered.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:Proprietary">
                <xs:annotation>
                    <xs:documentation>An identifier assigned under a proprietary scheme.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AgentNamePartTypeCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a type of an agent name part.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="AgentNameTypeCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a type of a person or organization name.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="onixPL:CommonName">
                <xs:annotation>
                    <xs:documentation>A name by which an agent is commonly known.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PositionInOrganization">
                <xs:annotation>
                    <xs:documentation>A name of a position which an agent holds within an organization.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:RegisteredName">
                <xs:annotation>
                    <xs:documentation>A name under which an agent is officially registered.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:TradingName">
                <xs:annotation>
                    <xs:documentation>A name under which an agent carries out business.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AgentPlaceRelatorCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a relationship between a defined agent or agent class and a place or place class.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="onixPL:GeographicalRestriction">
                <xs:annotation>
                    <xs:documentation>A territory or other geographical location within which a person or organization must be domiciled or affiliated.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PlaceOfCorporateRegistration">
                <xs:annotation>
                    <xs:documentation>The country or state where an organization is legally registered.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PrincipalPlaceOfBusiness">
                <xs:annotation>
                    <xs:documentation>The address at which a person or organization has a principal place of business.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:RegisteredAddress">
                <xs:annotation>
                    <xs:documentation>The officially registered address of an organization.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:SiteRestriction">
                <xs:annotation>
                    <xs:documentation>A site or sites at which a person or organization must be located. [Use in an Agent Definition, typically to specify a site restriction which applies to some or all authorized users.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AgentQuantityTypeCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a type of a quantity.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="AgentResourceRelatorCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a relationship between a defined agent or agent class and a resource or resource class.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="AgentTimePointRelatorCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a relationship between a defined agent or agent class and a time point or time point class.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="AgentTypeCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying whether an agent (or a member of an agent class) is a person or an organization, or whether it may be either of these.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="onixPL:Organization">
                <xs:annotation>
                    <xs:documentation>Agent is an organization or group of persons.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:Person">
                <xs:annotation>
                    <xs:documentation>Agent is a natural person.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:Unspecified">
                <xs:annotation>
                    <xs:documentation>Agent may be either a natural person or an organization or group.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="AnnotationTypeCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying the type of an annotation.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="onixPL:AcknowledgmentToInclude">
                <xs:annotation>
                    <xs:documentation>Text stating elements that must be included in an acknowledgement when an extract from licensed content is reproduced, but not precise wording.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:AcknowledgementWording">
                <xs:annotation>
                    <xs:documentation>Wording specified for an acknowledgment when an extract from licensed content is reproduced.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:AmbiguityInLicenseText">
                <xs:annotation>
                    <xs:documentation>Note pointing out an ambiguity in license wording.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ErrorInLicenseText">
                <xs:annotation>
                    <xs:documentation>Note pointing out an error in license wording, numbering or cross-referencing.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:FeesPayableNote">
                <xs:annotation>
                    <xs:documentation>Note detailing fees payable during the term of a license. [Use in a Payment Term only.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:Interpretation">
                <xs:annotation>
                    <xs:documentation>Note explaining an interpretation of license wording.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:NewTitleAccessNote">
                <xs:annotation>
                    <xs:documentation>Note explaining conditions relating to the addition of a newly published title to an existing license, typically when access is provided at no cost during an introductory period. [For use in a General Term of type 'ChangesToLicensedContent'.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PaymentNote">
                <xs:annotation>
                    <xs:documentation>Note on payment terms. [For use in a term that is not itself a Payment Term, but that carries a reference to a payment, for example when a Continuing Access Term refers to a payment for post-cancellation online access.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PostCancellationFileSupplyHoldingsNote">
                <xs:annotation>
                    <xs:documentation>Note detailing holdings for which files will be provided to a licensee for local use after cancellation of subscription(s). [Use in a Continuing Access Term to supplement information encoded with the relator ‘ApplicableTo’ in a Continuing Access Term Related Resource element.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PostCancellationFileSupplyNote">
                <xs:annotation>
                    <xs:documentation>Note detailing the format and/or method of supply for files which will be provided to a licensee for local use after cancellation of subscription(s). [Use in a Continuing Access Term only.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PostCancellationOnlineAccessHoldingsNote">
                <xs:annotation>
                    <xs:documentation>Note detailing the holdings for which post-cancellation online access will be provided, either by a licensor or by a trusted third party. [Use in a Continuing Access Term to supplement information encoded with the relator ‘ApplicableTo’ in a Continuing Access Term Related Resource element.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PostCancellationOnlineAccessSupplyNote">
                <xs:annotation>
                    <xs:documentation>Note detailing the method by which post-cancellation online access will be provided. [Use in a Continuing Access Term to supplement information encoded with the relator ‘ContinuingAccessProvider’ in a Continuing Access Term Related Agent element.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PriceBasisNote">
                <xs:annotation>
                    <xs:documentation>Note explaining factors on which price calculations are based, eg number of FTEs, overall spend, consortium membership.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PriceIncreaseCap">
                <xs:annotation>
                    <xs:documentation>Note specifying a restriction on year-on-year price increases, when not expressed as a simple percentage (eg if related to a published index of inflation). [See also Payment Term Quantity Type.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ReferencedText">
                <xs:annotation>
                    <xs:documentation>Text quoted from a referenced document other than a license document.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:SpecialConditions">
                <xs:annotation>
                    <xs:documentation>Note describing special conditions to which attention is drawn.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:TitleSubstitutionNote">
                <xs:annotation>
                    <xs:documentation>Note explaining conditions relating to the substitution of titles under an existing license. [For use in a General Term of type 'ChangesToLicensedContent'.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:TransferredTitleNote">
                <xs:annotation>
                    <xs:documentation>Note explaining conditions relating to the transfer of a subscribed title from another publisher to the licensor. [For use in a General Term of type 'ChangesToLicensedContent'.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:VariationsFromModel">
                <xs:annotation>
                    <xs:documentation>Note describing variations from a model license on which a license is based.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:ArchiveCopyFormat">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] Text or local values describing the form in which archival content would be delivered for continuing use after termination.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:CitationRequirementDetail">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] A specification of the required or recommended form of citation. Used only for an annotation to a License Grant.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:ConcurrentUserNote">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] Notes about the number of concurrent users permitted under a license. Used only for an annotation to a Usage Term of type 'Access'.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:DistanceEducationNote">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] Information which qualifies a permissions statement on Distance Education. Used only for an annotation to an Agent Definition for Authorized Users.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:DistanceEducationStatus">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] An annotation specifying whether use for Distance Education is permitted. The Annotation Text should comprise one of the values defined for Usage Term Status. Used only as an Annotation to an Agent Definition for Authorized Users.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:LocalText">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] Text used to define Authorized Users as an annotation to an Agent Definition when the ERMI Local Authorized User Definition flag has the value 'Y'.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:MaintenanceWindow">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] The recurring period of time reserved by the product provider for technical maintenance activities, during which online access may be unavailable. Used only for an annotation to a General Term of type 'Service Performance Guarantee'.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:Note">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] Information which qualifies a specific license term and populates the corresponding ERMI term note element. Used for an annotation in any ONIX-PL term that maps directly to and from a corresponding ERMI term.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:OtherUseRestrictionNote">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] Additional information about other use restrictions not adequately described elsewhere. Used only for an annotation to a License Grant.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:OtherUserRestrictionNote">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] Additional information about other user restrictions not adequately described elsewhere. Used only for an annotation to a License Grant.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:PerpetualAccessHoldings">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] The dates of coverage for which perpetual rights are available and agreed upon in the legal contract. Used only in a Continuing Access Term of type 'Continuing Online Access'.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:TerminationRequirementsNote">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] A clarification of the termination requirements and what certification of the requirement activities is necessary. Used only in a General Term of type 'Action On Termination'.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:TermsNote">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] Notes about the terms as a whole. Used only for an annotation to a License Grant.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:Text">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] Text used to specify a term in cases where ERMI defines a term content as 'Text'.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:UptimeGuarantee">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] The specific percentage of up-time guaranteed for the product being licensed, and the context for that percentage. Used only for an annotation to a General Term of type 'Service Performance Guarantee'.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:Value">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] An ERMI-defined value used to qualify a term, eg 'General / IP only / other' or 'No / All / Financial only / All but user terms'. Applicable values depend on the term type. Used only in General Terms of type 'Licensor Indemnity', 'Confidentiality Of Agreement', 'ERMI: Licensee Termination Right' and 'ERMI: Licensor Termination Right'.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ERMI:WalkInUserTermNote">
                <xs:annotation>
                    <xs:documentation>[Use only for mapping from an ERMI encoding.] Information which qualifies the status or permitted actions of Walk-In Users. Used only for an annotation to an Agent Definition for Authorized Users.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContinuingAccessTermAgentRelatorCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a relationship between a continuing access term and a related agent or agent class.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="onixPL:ContinuingAccessProvider">
                <xs:annotation>
                    <xs:documentation>The related agent is an organization responsible for providing continuing access (eg Licensor, LOCKSS, CLOCKSS, Portico). [May refer either to post-termination online access or to long-term preservation.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContinuingAccessTermPlaceRelatorCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a relationship between a continuing access term and a related place or place class.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="ContinuingAccessTermQuantityTypeCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a type of a continuing access term quantity.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="ContinuingAccessTermResourceRelatorCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a relationship between a continuing access term and a related resource or resource class.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="onixPL:ApplicableTo">
                <xs:annotation>
                    <xs:documentation>The continuing access term applies to the related resource(s).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="ContinuingAccessTermTimePointRelatorCode">
        <xs:annotation>
            <xs:documentation>A controlled value specifying a relationship between a continuing access term and a related time point or time point class.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>
    <xs:simpleType name="ContinuingAccessTermTypeCode">
        <xs:annotation>
            <xs:documentation>A controlled value that specifies a type of continuing access term.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="onixPL:ArchiveCopy">
                <xs:annotation>
                    <xs:documentation>DEPRECATED – use PostCancellationFileSupply and/or PreservationInDarkArchive as applicable. [Was defined as: A license term that specifies that a licensor will directly or indirectly provide the licensee with an archive copy of licensed content for continued use after termination of a license.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:ContinuingAccess">
                <xs:annotation>
                    <xs:documentation>DEPRECATED – use PostCancellationOnlineAccess. [Was defined as: A license term that specifies that a licensor will provide the licensee with continuing access to licensed content after termination of a license, either through its own online service or otherwise.]</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:NotificationOfDarkArchive">
                <xs:annotation>
                    <xs:documentation>A license term that requires the licensor to notify the location of any dark archive services where licensed content is deposited.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PostCancellationFileSupply">
                <xs:annotation>
                    <xs:documentation>A license term that specifies that on cancellation of a subscription a licensor will supply an electronic file of licensed content for continued use by a licensee.</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PostCancellationOnlineAccess">
                <xs:annotation>
                    <xs:documentation>A license term that specifies that a licensor will enable a licensee to have continuing online access to licensed content after cancellation of a subscription, either on the licensor’s own systems or by an arrangement with a third party (eg Portico PCA, LOCKSS).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="onixPL:PreservationInDarkArchive">
                <xs:annotation>
                    <xs:documentation>A license term that specifies that a licensor will ensure the long-term preservation of licensed content by continuously depositing it in a ‘dark archive’ held by a trusted third party (eg Portico, CLOCKSS, Koninklijke Bibliotheek).</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CurrencyCode">
        <xs:annotation>
            <xs:documentation>A three-letter currency code taken from ISO 4217: values are not listed here, but are included in the schema files. The onixPL: namespace prefix is NOT used.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="AED">
                <xs:annotation>
                    <xs:documentation>UAE Dirham</xs:documentation>
                    <xs:documentation>United Arab Emirates</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
            <xs:enumeration value="AFA">
                <xs:annotation>
                    <xs:documentation>Afghani</xs:documentation>
                    <xs:documentation>DEPRECATED, replaced by AFN</xs:documentation>
                </xs:annotation>
            </xs:enumeration>
