{
    "dynamic_templates": [
      {
        "date": {
          "match": "*date*",
          "match_mapping_type": "date",
          "mapping": {
            "type": "date",
            "format": "yyyy-MM-dd HH:mm:ss||yyyy-MM-dd'T'HH:mm:ss'Z'||epoch_millis"
          }
        }
      },
      {
        "year": {
          "match": "*year*",
          "match_mapping_type": "string",
          "mapping": {
            "fields": {
              "keyword": {
                "ignore_above": 256,
                "type": "keyword"
              }
            },
            "type": "text"
          }
        }
      },
      {
        "allFieldWithname": {
          "match": "*name",
          "unmatch": "object*",
          "match_mapping_type": "string",
          "mapping": {
            "fields": {
              "keyword": {
                "ignore_above": 256,
                "type": "keyword"
              }
            },
            "type": "text"
          }
        }
      },
      {
        "publisher": {
          "match": "publisher*",
          "match_mapping_type": "string",
          "mapping": {
            "type": "nested",
            "properties": {
              "id": { "type": "long" },
              "name": { "type": "keyword" }
            }
          }
        }
      },
      {
        "objectIDs": {
          "match": "*id",
          "unmatch": "g*",
          "match_mapping_type": "long",
          "mapping": {
            "index": false,
            "type": "long"
          }
        }
      }
    ],
