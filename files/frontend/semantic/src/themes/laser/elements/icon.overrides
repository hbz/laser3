/* Load & Define Laser Font */

@font-face {
  font-family: 'icomoon';
  src: url("../assets/fonts/icomoon.woff2") format('woff2'),url("../assets/fonts/icomoon.woff") format('woff');
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-decoration: inherit;
  text-transform: none;
}

// for the audit Icon that remains from the Audit Button in case of
.la-header-with-icon {
  .la-clone {
  vertical-align: super;
  }
}
/* Light Grey */
i.la-light-grey.icon {
  color: #e8e8e8 !important;
}
i.inverted.la-light-grey.icon {
  color: rgba(0,0,0,.6) !important;
}
i.inverted.bordered.la-light-grey.icon,
i.inverted.circular.la-light-grey.icon {
  background-color: #e8e8e8 !important;
  color: rgba(0,0,0,.6) !important;
}
// special for search icons
.circular.icon.la-license,
.circular.icon.la-package,
.circular.icon.la-subscription,
.circular.icon.la-organisation,
.circular.icon.la-platform,
.circular.icon.la-journal,
.circular.icon.la-database,
.circular.icon.la-ebook{
  display: inline-block;
  -webkit-box-shadow: 0em 0em 0em 0em rgba(0, 0, 0, 0) inset;
  box-shadow: 0em 0em 0em 0em rgba(0, 0, 0, 0) inset;
}
// for all the icons
// ****  License  **** //
.icon.la-license {
  background-color: @license;
}
.icon.la-license:before {
  content: "\f24e";
  color: white;
}

