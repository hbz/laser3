i.inverted.circular.icon.la-filter-icon {
  background-color: #8699b1!important;
}
.la-rotate {
  transform: translateY(3px) rotate(90deg);
}

// Child Subscription
.la-subscriptionIsChild {
  position: absolute;
  left: 24px;
  top: -15px;
  background-color: white;
  font-size: .6em!important;
  box-shadow: 0 0 0 0.1em #fa820a inset!important;
  padding: 0em;
  transition: all 0.5s ease;
}
/* change style when page scrolled down*/
i.orange.icon.la-subscriptionIsChild.fixed {
  position: fixed;
  margin-left: 150px!important;
  background-color:transparent!important;
  color: gray!important;
  box-shadow: none!important;
  transition: all 1s ease;
}
@media only screen and (max-width: @computerBreakpoint  ) {
  i.orange.icon.la-subscriptionIsChild.fixed {
    display: none;
  }
}

// My Object
.la-objectIsMine {
  position: absolute;
  left: 24px;
  top: -15px;
  background-color: white;
  font-size: .6em!important;
  padding: 0em;
  transition: all 0.5s ease;
}
/* change style when page scrolled down*/
i.icon.la-objectIsMine.fixed {
  position: fixed;
  margin-left: 205px!important;
  background-color:transparent!important;
  color: gray!important;
  box-shadow: none!important;
  transition: all 1s ease;
}
@media only screen and (max-width: @computerBreakpoint  ) {
  .ui.icon.header i.icon.la-objectIsMine.fixed {
    display: none;
  }
}


.la-icon-absolute {
  position: absolute;
  z-index: 100;
  left: 0;
  right: 0;
}
