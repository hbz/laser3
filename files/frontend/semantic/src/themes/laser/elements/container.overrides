/*******************************
         Theme Overrides
*******************************/

/* Laser Breakpoint */
@media only screen and (min-width: @laser-breakpoint) {
  .ui.ui.ui.container:not(.fluid) {
    width: 1440px;
    margin-left: auto;
    margin-right: auto;
  }
}
/* Login Card in Mobiles */
@media only screen and (max-width: @tabletBreakpoint) {
  #login .ui.card {
    margin-top: 80px;
  }
}

